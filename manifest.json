{
  "manifest_version": 2,
  "short_name": "urlando",
  "name": "Urlando",
  "version": "2.0.0",
  "author": "Dica-Developer",
  "description": "Cycles through user defined URLs in user defined intervals. Useful as dashboard / wallboard.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "offline_enabled": true,
  "permissions": [
    "storage",
    "webview"
  ]
}
